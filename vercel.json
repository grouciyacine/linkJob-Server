{
    
    "version":2,
    "name":"linkjob-server",
    "builds":[
        {
            "src":"./index.js",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"/",
            "methods":["GET","POST","PUT","DELETE","PATCH","OPTIONS"]
        }
    ]
}
